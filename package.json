{
  "name": "querysearch",
  "displayName": "querySearch",
  "description": "%description%",
  "version": "0.0.8",
  "engines": {
    "vscode": "^1.92.0"
  },
  "publisher": "yuyakawakamy",
  "repository": {
    "url": "https://github.com/ykawakamy/querySearch"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onFileSystem:memfs"
  ],
  "main": "./dist/extension.js",
  "l10n": "./l10n",
  "icon": "icon.png",
  "contributes": {
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "querySearch",
          "name": "%QueryPanel%"
        }
      ]
    },
    "commands": [
      {
        "command": "querySearch.openFile",
        "title": "%openFile%",
        "icon": "$(go-to-file)"
      },
      {
        "command": "querySearch.previewFile",
        "title": "%openFile%",
        "icon": "$(go-to-file)"
      },
      {
        "command": "querySearch.replace",
        "title": "%replace%",
        "icon": "$(search-replace)"
      },
      {
        "command": "querySearch.replaceAll",
        "title": "%replaceAll%",
        "icon": "$(search-replace-all)"
      },
      {
        "command": "querySearch.replaceFiles",
        "title": "%replaceFiles%",
        "icon": "$(search-replace-all)"
      },
      {
        "command": "querySearch.copyResult",
        "title": "%copyResult%"
      },
      {
        "command": "treeviewWrap:querySearch.copyResult",
        "title": "%copyResult%"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "querySearch.openFile",
          "when": "view == searchResult && viewItem == querySearch.result && querysearch.replaceMode == true",
          "group": "inline"
        },
        {
          "command": "querySearch.replace",
          "when": "view == searchResult && viewItem == querySearch.result && querysearch.replaceMode == true",
          "group": "inline"
        },
        {
          "command": "querySearch.openFile",
          "when": "view == searchResult && viewItem == querySearch.file && querysearch.replaceMode == true",
          "group": "inline"
        },
        {
          "command": "querySearch.replaceAll",
          "when": "view == searchResult && viewItem == querySearch.file && querysearch.replaceMode == true",
          "group": "inline"
        },
        {
          "command": "querySearch.copyResult",
          "when": "view == searchResult"
        }
      ],
      "webview/context": [
        {
          "command": "treeviewWrap:querySearch.copyResult",
          "when": "webviewId == querySearch && view == searchResult"
        }
      ],
      "view/title": [
        {
          "command": "querySearch.replaceFiles",
          "when": "view == searchResult",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "node scripts/build.mjs",
    "watch": "node scripts/build.mjs --watch",
    "package": "npx tsc ",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node --enable-source-maps ./out/test/runTest.js",
    "l10n:generate": "npx @vscode/l10n-dev export --outDir ./l10n ./src"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.17.7",
    "@types/minimatch": "^5.1.2",
    "@types/mocha": "^10.0.7",
    "@types/node": "~20.16.1",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "@types/scss-parser": "^1.0.4",
    "@types/vscode": "^1.92.0",
    "@types/vscode-webview": "^1.57.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vscode/l10n-dev": "^0.0.34",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.0.0",
    "esbuild": "0.21.4",
    "eslint": "^8.57.0",
    "glob": "^10.4.5",
    "mocha": "^10.7.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@react-hook/event": "^1.2.6",
    "@vscode/codicons": "^0.0.35",
    "@vscode/l10n": "^0.0.18",
    "css-select": "^5.1.0",
    "css-what": "^6.1.0",
    "html-parser": "link:..\\html-parser",
    "ignore": "^5.3.2",
    "lodash": "^4.17.21",
    "minimatch": "^10.0.1",
    "react": "link:..\\vscode-webview-treeview\\treeview-onwebview\\node_modules\\react",
    "react-dom": "link:..\\vscode-webview-treeview\\treeview-onwebview\\node_modules\\react-dom",
    "scss-parser": "^1.0.6",
    "treeview-on-vscode-webview": "link:..\\vscode-webview-treeview\\treeview-onwebview",
    "vscode-uri": "^3.0.8"
  },
  "pnpm": {
    "overrides": {
      "react": "../vscode-webview-treeview/treeview-onwebview/node_modules/react",
      "react-dom": "../vscode-webview-treeview/treeview-onwebview/node_modules/react-dom",
      "@react-hook/event": "../vscode-webview-treeview/treeview-onwebview/node_modules/@react-hook/event"
    }
  }
}